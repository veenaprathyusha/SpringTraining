<?xml version="1.0" encoding="UTF-8"?>

<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="student" class="com.training.org.Student">  
<property name="name" value="veena"></property>
  <property name="address" value="gnt"></property>
  <property name="numCourses" value="1"></property>
  <property name="grades" value="25"></property>
  
<property name="courses" value="Angular"></property>
</bean>  
 
 
 <bean id="teacher" class="com.training.org.Teacher">  
<property name="name" value="veena"></property>
  <property name="address" value="gnt"></property>
  <property name="numCourses" value="1"></property>
  
<property name="courses" value="python"></property>
</bean>  
  
</beans> 
